{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "react-app",
        "prettier","airbnb"
    ],
    "parserOptions": {
        "ecmaVersion": 12 
    },
    "plugins": [
        "react", "prettier"
    ],
    "rules":  {
        "quotes":"off",
        "react/prefer-stateless-function":"off",
        "comma-dangle":"off",
        "linebreak-style":"off",
        "object-curly-newline":"off",
        "react/jsx-filename-extension":"off",
        "react/prop-types":"off",
        "no-useless-constructor":"off",
        "react/button-has-type":"off",
        "operator-linebreak": "off",
        // "eslint object-curly-newline": ["error", { "consistent": false }],
        "prettier/prettier": [
          "error",
          {
            
            // "printWidth": 80,
            // "trailingComma": "es5",
            "endOfLine": "auto"
            // "jsxSingleQuote": false,
            // "singleQuote": false,
            // "useTabs": false
          }
        ]
      }
}
